{
  "structure": {
    "shard": "~",
    "kind": "#DUAL",
    "required-ctx": null,
    "required-idx": null,
    "opens-ctx": null,
    "ctx-persists": false,
    "ctx-clashes": false,
    "variables": {},
    "regex-open": "~",
    "html-open": "~",
    "html-replaces": "#AUTO",
    "regex-close": "#AUTO",
    "html-close": "#AUTO"
  },

  "part":
  {
    "id": "link-text",
    "kind": "#DUAL",
    "require-context": null,
    "open-context": "table.header",
    "context-persists": false,
    "variables": ["displayed", "hyperlink"],
    "regex-match": "[.*](.*)",
    "replace-text": "<a href={0}>",
    "html-replace": "#ALL"
  },

  "line": {
    "shard": "table-header",
    "kind": "#POLY",
    "required-ctx": null,
    "open-context": "table.header",
    "regex-open": "| {} |",
    "open-html": "<table>",
    "html-replace": "#AUTO",
    "close-regex": "#LINE.BLANK",
    "close-html": "</table>"
  }
}
